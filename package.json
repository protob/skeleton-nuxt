{
  "name": "skeleton-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .eslintignore ",
    "lint": "yarn lint:js --fix",
    "test": "jest",
    "cc": "node ./generateComponent.js",
    "cy:open": "npx cypress open",
    "test:unit": "jest --no-cache",
    "test:unitCI": "jest --no-cache --updateSnapshot",
    "test:coverage": "jest --no-cache --coverage",
    "test:resetsnapshots": "jest --no-cache -u",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nextindex/next-scss": "^1.2.1",
    "@nuxt/content": "^1.11.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.24.5",
    "@nuxtjs/pwa": "^3.3.5",
    "autoprefixer": "^10",
    "core-js": "^3.15.1",
    "date-fns": "^2.21.3",
    "glightbox": "^3.0.9",
    "imagesloaded": "^4.1.4",
    "mobile-device-detect": "^0.4.3",
    "nuxt": "^2.15.7",
    "nuxt-gsap": "^0.1.4",
    "nuxt-i18n": "^6.27.2",
    "snapsvg": "^0.5.1",
    "tiny-cookie": "^2.3.2",
    "vee-validate": "^3.4.10",
    "vue-cookie-law": "^1.13.3",
    "vue-sweetalert2": "^5.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.3",
    "@nuxt/components": "2.1.8",
    "@nuxt/image": "^0.5.0",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@types/jest": "^26.0.23",
    "@vue/test-utils": "^1.2.1",
    "agen": "^1.0.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "gsap": "^3.7.0",
    "husky": "^7.0.1",
    "import-postcss": "^8.0.2",
    "imports-loader": "^3.0.0",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.0.3",
    "postcss": "^8.3.5",
    "postcss-assets": "^6.0.0",
