{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom",
      "dom.iterable"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/image",
      "@nuxt/types",
      "@nuxt/content",
      "@nuxtjs/axios",
      "@types/node",
      "nuxt-i18n",
      "@types/jest"
    ]
  },
  "typeRoots": [
    "./types"
  ],
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}